{
  "name": "randock/anti-captcha-client",
  "description": "A PHP library to connect to https://anti-captcha.com",
  "type": "library",
  "authors": [
    {
      "name": "Jop Peters",
      "email": "jop@randock.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "Randock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Randock\\": "tests/"
    }
  }
}